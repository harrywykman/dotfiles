alias tootle="flatpak run com.github.bleakgrey.tootle"
alias scuttlebutt="/usr/bin/ssb-patchwork-3.16.2-x86_64.AppImage"
alias dice="xkcdpass --delimiter='-'"
alias wa="watson"
alias config='/usr/bin/git --git-dir=/home/archaeon/.cfg/ --work-tree=/home/archaeon'
alias config-priv='/usr/bin/git --git-dir=/home/archaeon/.cfg_private/ --work-tree=/home/archaeon'
alias music-mnt='sshfs -o idmap=user library:/home/archaeon/music /home/archaeon/music/'
alias relock="xautolock -time 10 -locker 'i3lock -d -c 000000' &"
alias lock="xautolock -enable"
alias unlock="xautolock -disable"
alias mount_music="sshfs -o idmap=user library:/home/archaeon/music /home/archaeon/music"
alias mount_music_mess="sshfs -o idmap=user library:/home/archaeon/music_mess /home/archaeon/music_mess"
alias mount_music_ip="sshfs -o idmap=user 192.168.1.165:/home/archaeon/music /home/archaeon/music"
